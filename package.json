    "prettier": "prettier --write \"**/*.{js,md,graphql,json}\"",
    "precommit": "precise-commits"
  },
  "dependencies": {
    "apollo-boost": "^0.1.6",
    "compression": "^1.7.2",
    "graphql-tag": "^2.9.2",
    "graphql-yoga": "^1.13.1",
    "isomorphic-unfetch": "^2.0.0",
    "next": "^6.0.3",
    "prisma-binding": "^2.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "casual": "^1.5.19",
    "cuid": "^2.1.1",
    "husky": "^0.14.3",
    "precise-commits": "^1.0.2",
    "prettier": "1.12.1"
